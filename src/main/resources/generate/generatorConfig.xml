<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="Mysql" targetRuntime="MyBatis3">
        <property name="javaFileEncoding" value="UTF-8"/>
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>
        <plugin type="tk.mybatis.mapper.generator.MapperPlugin">
            <property name="mappers" value="tk.mybatis.mapper.common.Mapper"/>
            <!-- caseSensitive默认false，当数据库表名区分大小写时，可以将该属性设置为true -->
            <property name="caseSensitive" value="false"/>
        </plugin>
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://139.224.118.49:3306/cctest02"
                        userId="root"
                        password="ohayou007">
        </jdbcConnection>

        <!--POJO 路径 TODO 每个开发需要手动配置-->
        <javaModelGenerator targetPackage="com.cc.ccbootdemo.facade.domain.dataobject"
                            targetProject="G:\iwspace\cc\cc-boot-demo\src\main\java"/>

        <!--sql mapper 路径 TODO 每个开发需要手动配置-->
        <sqlMapGenerator targetPackage="mapper"
                         targetProject="G:\iwspace\cc\cc-boot-demo\src\main\resources"/>

        <!--mapper dao 路径 TODO 每个开发需要手动配置-->
        <javaClientGenerator targetPackage="com.cc.ccbootdemo.core.mapper"
                             targetProject="G:\iwspace\cc\cc-boot-demo\src\main\java"
                             type="XMLMAPPER"/>


        <!--需要自动生成的表-->

        <table tableName="complaints" domainObjectName="ComplaintsDO"/>

<!---->
    </context>
</generatorConfiguration>